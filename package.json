{
	"name": "Shikaku",
	"type": "module",
	"version": "0.0.1",
	"description": "A site of online Shikaku game.",
	"main": "server/server.js",
	"scripts": {
		"dev:server": "node server/server.js",
		"dev:client": "vite --config vite.config.js",
		"dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
		"build": "vite build --config vite.config.js",
		"preview": "vite preview --config vite.config.js",
		"start": "NODE_ENV=production node server/server.js"
	},
	"repository": "https://github.com/gbCzz/Shikaku.git",
	"author": "Czz <Eason0208@163.com>",
	"license": "MIT",
	"dependencies": {
		"@koa/cors": "^5.0.0",
		"@koa/router": "^12.0.1",
		"axios": "^1.13.2",
		"koa": "2.16.2",
		"koa-bodyparser": "^4.4.1",
		"koa-static": "^5.0.0",
		"vue": "^3.5.23"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.5.0",
		"vite": "^5.0.0",
		"concurrently": "^8.2.0"
	}
}
